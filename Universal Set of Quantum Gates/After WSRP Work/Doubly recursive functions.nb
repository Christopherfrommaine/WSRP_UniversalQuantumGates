 RowBox[{
  RowBox[{"matFromSequence", "[", "L_List", "]"}], " ", ":=", " ", 
  RowBox[{"Transpose", "[", 
   RowBox[{"Table", "[", "\n", "  ", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"And", "[", 
        RowBox[{
         RowBox[{"n", " ", ">", " ", "0"}], ",", " ", 
         RowBox[{"n", " ", "<=", " ", 
          RowBox[{"Length", "[", "L", "]"}]}]}], "]"}], ",", 
       "\[IndentingNewLine]", "    ", 
       RowBox[{"UnitVector", "[", 
        RowBox[{
         RowBox[{"Length", "[", "L", "]"}], ",", " ", "n"}], "]"}], ",", 
       "\[IndentingNewLine]", "    ", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{"0", ",", " ", 
