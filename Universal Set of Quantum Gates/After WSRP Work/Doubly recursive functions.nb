Cell[BoxData[
 RowBox[{
  RowBox[{"matMtry2", "[", "d_", "]"}], ":=", 
  RowBox[{"matFromSequence", "[", 
   RowBox[{"Table", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"Or", "[", 
        RowBox[{
         RowBox[{"j", "==", "1"}], ",", 
         RowBox[{"j", "==", "2"}]}], "]"}], ",", " ", "1", ",", " ", 
       "\[IndentingNewLine]", 
       RowBox[{"j", "-", 
        RowBox[{"Dot", "[", 
         RowBox[{
          RowBox[{"Range", "[", "d", "]"}], ",", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"With", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"prevMatM", "=", 
                RowBox[{"matMtry2", "[", 
                 RowBox[{"d", "-", "2"}], "]"}]}], "}"}], ",", 
              RowBox[{
               RowBox[{"matE", "[", 
                RowBox[{"2", ",", " ", 
                 RowBox[{"d", "-", "2"}]}], "]"}], ".", "prevMatM", ".", 
               "prevMatM", ".", 
               RowBox[{"matD", "[", 
                RowBox[{"2", ",", " ", "d"}], "]"}]}]}], "]"}], ")"}], ".", 
           RowBox[{"UnitVector", "[", 
            RowBox[{"d", ",", " ", "j"}], "]"}]}]}], "]"}]}]}], 
      "\[IndentingNewLine]", "]"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"j", ",", " ", "1", ",", " ", "d"}], "}"}]}], "]"}], 
   "]"}]}]], "Input",
